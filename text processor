#include <stdio.h>

int text_processor(const char *text)
{
    int total = 0;
    int sign = 1;

    for (int i = 0; text[i] != '\0'; i++)
    {
        char c = text[i];

        if (c >= 'A' && c <= 'Z')
        {
            total += 10 * sign;
        }
        else if (c >= '0' && c <= '9')
        {
            int digit = c - '0';
            if (digit % 2 == 0)
                sign = 1;
            else
                sign = -1;
        }
        else if (c == '!')
        {
            total = 0;
        }
        printf("%d\n", total);
    }

    return total;
}

int main() {
    char sample_text[100];
    scanf("%99s", sample_text);
    int result = text_processor(sample_text);
    printf ("%d\n", result);
    return 0;
}
